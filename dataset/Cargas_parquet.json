{
	"name": "Cargas_parquet",
	"properties": {
		"linkedServiceName": {
			"referenceName": "serv_vinculado_datalake",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"file_name": {
				"type": "string"
			},
			"folder": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "Parquet",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@concat(dataset().file_name, '_', formatDateTime(utcNow()))",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@dataset().folder",
					"type": "Expression"
				},
				"fileSystem": "input"
			},
			"compressionCodec": "snappy"
		},
		"schema": []
	}
}